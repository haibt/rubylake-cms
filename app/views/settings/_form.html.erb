<%= form_for @setting , :as => :setting,:method => :post, :html => { :class => "setting_form", :id => "setting_form" } do |f| %>
<div class="field">
	<%= f.label :title %>
	<%= f.text_field :title %>
</div>
<div class="field">
	<%= f.label :web_name %>
	<%= f.text_field :web_name %>
</div>

<div class="field">
	<%= f.label :website %>
	<%= f.text_field :website %>
</div>
<div class="field">
	<%= f.label :email %>
	<%= f.text_field :email %>
</div>
<div class="field">
	<p>Logo</p>
<%= f.file_field :logo%>
</div>
<div class="field">
	<%= f.label :about_us %>
	<%= f.cktext_area :about_us %>
</div>

<div class="field">
	<%= f.submit :class => "btn btn-primary"%>
</div>
<% end %>
