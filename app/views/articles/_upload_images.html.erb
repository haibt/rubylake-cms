<% if current_user%>
<div class="upload_img"> <b> Upload image</b>
  <%= form_for( @images, :as => 'image', :url => "/images/uploadFile", :html => {:id => "upload_form"} ) do |f| %>
    <%= hidden_field_tag "image[article_id]", @article.id %>
    <%= f.file_field :photo%><br />
	 <%= f.submit :class => "btn btn-primary"%>
  <%end%>
  <%end%>
</div>
