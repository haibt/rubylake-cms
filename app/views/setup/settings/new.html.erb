<%= form_for @setting, :url => 'create'  do |f| %>
	<div class="field">
		<%= f.label :title %>
		<br/>
		<%= f.text_field :title %>
	</div>
	<div class="field">
		<%= f.label :web_name %>
			<br/>
		<%= f.text_field :web_name %>
	</div>
	<div class="field">
		<%= f.label :email %>
			<br/>
		<%= f.text_field :email %>
	</div>
	<div class="field">
		<%= f.label :website %>
			<br/>
		<%= f.text_field :website %>
	</div>
	<div>
		<p>Logo:</p>
	<%= f.file_field :logo %><br />
	</div>
	<div class="field">
		<%= f.label :about_us %>
			<br/>
		<%= f.cktext_area :about_us %>
	</div>
	<div class="field">
		<%= f.submit :class => "btn btn-primary" %>
	</div>
  <% end %>


